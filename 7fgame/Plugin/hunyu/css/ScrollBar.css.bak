QTableWidget {
    background-color: rgb(28, 23, 18, 255);
}

QCheckBox  {
    spacing: 4px;
}

QCheckBox:enabled
{
    color:#e8cc6a;
}

QCheckBox::indicator:disabled
{
    color:#050505;
}

QCheckBox::indicator {
    width: 19px;
    height: 19px;
}

QCheckBox::indicator:unchecked {
    border-image: url(./Images/checkbox.png) 0 0 0 0 repeat repeat;
    border-width: 0px;
    border-radius: 0px;
}

QCheckBox::indicator:unchecked:hover {
    border-image: url(./Images/checkbox.png) 0 0 0 19  repeat repeat;
}

QCheckBox::indicator:unchecked:pressed {
    border-image: url(./Images/checkbox.png) 0 0 0 38 repeat repeat;
}

QCheckBox::indicator:checked {
    border-image: url(./Images/checkbox.png) 0 0 0 57 repeat repeat;
    border-width: 0px;
    border-radius: 0px;
}

QCheckBox::indicator:checked:hover {
    border-image: url(./Images/checkbox.png) 0 0 0 76 repeat repeat;
}

QCheckBox::indicator:checked:pressed {
    border-image: url(./Images/checkbox.png) 0 0 0 95 repeat repeat;
}    

QRadioButton::indicator {
    width: 20px;
    height: 15px;
}

QRadioButton::indicator:unchecked {
    border-image: url(./Images/radiobutton.png) 0 0 0 0 repeat repeat;
    border-width: 0px;
    border-radius: 0px;
}

QRadioButton::indicator:unchecked:hover {
