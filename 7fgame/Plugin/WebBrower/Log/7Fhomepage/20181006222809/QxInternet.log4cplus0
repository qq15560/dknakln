######################################################################## 
#rootLogger 
#	logger 
#		appender 
#			layout 
#				ConversionPattern 
#			filters 
######################################################################## 
log4cplus.rootLogger=TRACE, STDOUT 

######################################################################## 
#Logger Default这里简写为Internet 
#这样日志记录中记录Logger名时可以节省日志文件大小 
######################################################################## 
#log4cplus.logger.Internet=TRACE, AppenderNameRollingFile_Internet, AppenderNameDefault 
log4cplus.logger.Internet=unknown, AppenderNameRollingFile_Internet 
log4cplus.additivity.Internet=FALSE 

log4cplus.appender.STDOUT=log4cplus::ConsoleAppender 
log4cplus.appender.STDOUT.layout=log4cplus::PatternLayout 
log4cplus.appender.STDOUT.layout.ConversionPattern=%D{%m/%d/%y %H:%M:%S} [%t] %-5p %c - %m %n 
log4cplus.appender.STDOUT.logToStdErr=true 
log4cplus.appender.STDOUT.ImmediateFlush=true 

log4cplus.appender.AppenderNameRollingFile_Internet=log4cplus::RollingFileAppender 
log4cplus.appender.AppenderNameRollingFile_Internet.File=D:\起凡\7fgame\plugin\WebBrower\Log\7Fhomepage\20181006222809\QxInternetLog.log0 
#log4cplus.appender.AppenderNameRollingFile_Internet.MaxFileSize=5MB 
log4cplus.appender.AppenderNameRollingFile_Internet.MaxFileSize=1024KB 
log4cplus.appender.AppenderNameRollingFile_Internet.MaxBackupIndex=15 
#log4cplus.appender.AppenderNameRollingFile_Internet.layout=log4cplus::TTCCLayout 
log4cplus.appender.AppenderNameRollingFile_Internet.layout=log4cplus::PatternLayout 
log4cplus.appender.AppenderNameRollingFile_Internet.layout.ConversionPattern=%D{%m/%d/%y %H:%M:%S:%q} [%t] %-5p %c - %F(%L) %m %n 
log4cplus.appender.AppenderNameRollingFile_Internet.ImmediateFlush=true 
log4cplus.appender.AppenderNameRollingFile_Internet.BufferSize=0 
