	<Plugin ModuleId="[13]"			Name="EDAModuleDemo3">EDAModuleDemo3.dll</Plugin>
	<Plugin ModuleId="[14]"			Name="SingleChatPlugin">SingleChat.dll</Plugin>
	<Plugin ModuleId="[15]"			Name="MsgRecordPlugin">MsgRecord.dll</Plugin>
	<Plugin ModuleId="[16]"			Name="SysSettingPlugin">SysSetting.dll</Plugin>
	<Plugin ModuleId="[17, 18]"		Name="SysMessagePlugin">SysMessage.dll</Plugin>
	-->
</KITPlugins>




